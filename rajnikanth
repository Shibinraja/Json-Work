var object = {name:"RajiniKanth",age:33,hasPets:false};
var arr =[]

function printAllValues(){
	var object = {"name":"RajiniKanth","age":33,"hasPets":false};
	var arr =[]

	for (i in object){
		hey=object[i]
		arr.push(hey)
	}
console.log(arr)
}
printAllValues()

////////////////////////////////////

function Allkeys(){
	var say = {"name":"RajiniKanth","age":33,"hasPets":false};
	var arr =[]

	for (i in say){
	arr.push(i)
	}
console.log(arr)	
}
Allkeys()

