var expected = {foo:5, bar:6};
var actual = {foo:6, bar:6}

exp = JSON.stringify(expected)
act = JSON.stringify(actual)

if ((JSON.stringify(expected)) == (JSON.stringify(actual))){
console.log("Passed")}

else{
console.log("Failed Expected" +exp+ "but got" +act)} 

